{
    "name": "Jazz",
    "description": "Start chatting with an IT professional now.",
    "version": "13",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Jazz",
        "default_icon": "img/jazz-51.png",
        "default_popup": "src/extension/pop-up.html"
    },
    "permissions": [
        "identity",
        "identity.email",
        "tabs",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "page": "src/extension/background.html"
    },
    "web_accessible_resources": [
        "src/app/livechat.html",
        "src/extension/tracking.js"
    ],
    "content_security_policy": "script-src 'self' https://*.opendns.com https://*.livechatinc.com https://unpkg.com/@livechat/; object-src 'self'"
}