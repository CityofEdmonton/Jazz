{
    "name": "VIP Chat",
    "description": "Start chatting with an IT professional now.",
    "version": "1",
    "manifest_version": 2,
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "img/icon-34.png"
    },
    "permissions": [
    ],
    "content_scripts": [
        {
          "matches": ["*://*/*"],
          "js": ["src/extension/init.js"]
        }
      ], 
    "web_accessible_resources": [
        "src/app/livechat.html"
    ],
    "content_security_policy": "script-src 'self'; object-src 'self'"
}
